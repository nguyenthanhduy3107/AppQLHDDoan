@model List<Mode.selectALL_Chidoan1_Result>
@{
    ViewBag.Title = "Index";
    Layout = "~/Areas/Admin/Views/Shared/Layout.cshtml";
}

<h2>Index</h2>
<script> CKEDITOR, relace(ckeditor1);</script>
<div class="form-group"><textarea class="ckeditor" id="ckeditor1"></textarea></div>
<div class="row">
    <div class="col-lg-12">
        <div class="panel panel-default">
            <div class="panel-heading">
                THÔNG TIN ĐOÀN VIÊN
            </div>
            <!-- /.panel-heading -->
            <div class="panel-body">
                <table width="100%" class="table table-striped table-bordered table-hover" id="dataTables-example">
                    <thead>
                        <tr>
                            <th>Tên chi đoàn</th>
                            <th>Chi đoàn</th>
                            
                           
                        </tr>
                    </thead>
                    <tbody>
                        @foreach (var item in Model)
                        {
                            <tr class="odd gradeX">
                                <td>@item.chidoan</td>
                                <td>@item.tenchidoan</td>
                               
                               


                                
                                <td class="center">
                                    @Html.ActionLink("Sửa", "Edit", new { id = item.chidoan })
                                    @Html.ActionLink("Xóa", "Delete", new { id = item.chidoan }, new { onclick = "return confirm('ban co thuc su muon xoa " + item.tenchidoan + " khong');" })
                                </td>
                            </tr>
                                    }

                    </tbody>
                </table>
                <!-- /.table-responsive -->
            </div>
            <!-- /.panel-body -->
        </div>
        <!-- /.panel -->
    </div>
    <!-- /.col-lg-12 -->
</div>

